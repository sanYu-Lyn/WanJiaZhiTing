/* pages/fee_bill/fee_bill.wxss */
.item_container {
  display: flex;
  flex-direction: row;
  margin: 30rpx 30rpx 0 30rpx;
}

.item_left {
  flex: 9;
  height: 100%;
  padding: 10rpx 30rpx 10rpx 30rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}

.item_right {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}

.item_title {
  font-size: 30rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #5C5C5C;
}

.item_date {
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 300;
  color: #5C5C5C;
}

.item_num {
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 300;
  color: #5C5C5C;
  word-break: break-all;
}

.item_pay {
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 300;
  color: #5C5C5C;
}

.item_result {
  font-size: 30rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #A2A4A4;
  -webkit-writing-mode: vertical-rl;
  writing-mode: vertical-rl;
}

.item_result_wait {
  font-size: 30rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #6EC7FF;
  -webkit-writing-mode: vertical-rl;
  writing-mode: vertical-rl;
}