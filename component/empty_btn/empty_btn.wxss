/* component/empty/empty.wxss */

.empty_container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.empty_img {
  width: 302rpx;
  height: 302rpx;
  margin-top: 80rpx;
}

.empty_text {
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 300;
  color: #A2A3A3;
}

.submit_container {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 80rpx;
}

.submit {
  width: 400rpx;
  height: 80rpx;
  background: #FFFFFF;
  border-radius: 40rpx;
  font-size: 30rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #5C5C5C;
  display: flex;
  justify-content: center;
  align-items: center;
}